{
  "name": "resy",
  "version": "11.0.0-rc.2",
  "description": "React State Easy",
  "repository": "https://github.com/lsbFlying/resy.git",
  "author": "liushanbao <1262300490@qq.com>",
  "homepage": "https://github.com/lsbFlying/resy",
  "bugs": "https://github.com/lsbFlying/resy/issues",
  "main": "index.js",
  "module": "dist/esm/index.js",
  "types": "dist/resy.d.ts",
  "files": [
    "index.js",
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/resy.d.ts",
        "node": {
          "production": "./dist/esm/resy.prod.js",
          "development": "./dist/esm/resy.js",
          "default": "./dist/esm/index.js"
        },
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/resy.d.ts",
        "node": {
          "production": "./dist/resy.cjs.prod.js",
          "development": "./dist/resy.cjs.js",
          "default": "./index.js"
        },
        "default": "./index.js"
      }
    },
    "./dist/esm": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "module": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./dist/umd": {
      "import": {
        "types": "./dist/umd/index.d.ts",
        "default": "./dist/umd/index.js"
      },
      "module": {
        "types": "./dist/umd/index.d.ts",
        "default": "./dist/umd/index.js"
      },
      "default": {
        "types": "./dist/umd/index.d.ts",
        "default": "./dist/umd/index.js"
      }
    },
    "./dist/system": {
      "import": {
        "types": "./dist/system/index.d.ts",
        "default": "./dist/system/index.js"
      },
      "module": {
        "types": "./dist/system/index.d.ts",
        "default": "./dist/system/index.js"
      },
      "default": {
        "types": "./dist/system/index.d.ts",
        "default": "./dist/system/index.js"
      }
    }
  },
  "scripts": {
    "test": "vitest run --coverage --environment jsdom",
    "test:dev": "vitest --ui --coverage --environment jsdom",
    "build": "rollup -c --configPlugin @rollup/plugin-typescript && node createEntryFile.js",
    "prepublishOnly": "rm -rf dist coverage && npm test && npm run build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest",
    "access": "public"
  },
  "dependencies": {
    "use-sync-external-store": "^1.2.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.2.1",
    "@types/node": "^18.19.18",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@types/react-native": "^0.71.13",
    "@types/rollup-plugin-auto-external": "^2.0.5",
    "@types/use-sync-external-store": "0.0.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^21.1.2",
    "node": "^19.8.1",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.71.16",
    "rollup": "^3.29.4",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-dts": "^5.3.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": "^12.20.0 || ^14.18.0 || >=16.0.0"
  },
  "packageManager": "pnpm@8.0.0",
  "eslintIgnore": [
    "dist"
  ],
  "license": "MIT",
  "keywords": [
    "reactive",
    "reactive-state",
    "react",
    "hooks",
    "flux",
    "redux",
    "store",
    "state",
    "resy",
    "proxy",
    "manager",
    "management"
  ]
}
